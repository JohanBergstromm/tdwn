<div class="col-12">
	<h2 class="news-title">{{title}}</h2>
</div>
<div class="d-flex flex-wrap">
	{{#each data}}
	<div class="col-12 col-md-6 d-flex">
		<a href="{{this.url}}" target="_blank" class="news-item p-0 d-flex flex-wrap w-100">
		    <div class="img" style="background-image: url('{{this.urlToImage}}')"></div>
		    <div class="content">
		        <h3>{{this.title}}</h3>
		        <h6><time class="timeago" datetime="{{this.publishedAt}}"></time></h6>
		        <p>{{this.description}}</p>
		    </div>
		</a>
	</div>
	{{/each}}
</div>